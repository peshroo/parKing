<div class="listings_index">
  <%= link_to 'New Listing', new_listing_path %>
  <h1>All listings</h1>
  <div class="listings_container">    
    <% @listings.each do |listing| %>
      <div class="listing">
        <h3><%= link_to listing.name, listing %></h3>
        Location: <%= listing.location %><br>
        Description: <%= listing.description %><br>
        Price: <%= listing.price %><br>
        Rating: <%= listing.rating %><br>
        Available Time: <%= listing.start.strftime('%b %e, %l:%M %p') %>-<%= listing.end.strftime('%b %e, %l:%M %p') %>
      </div>
    <% end %>
  </div>
</div>
