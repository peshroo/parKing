<%= form_for([@listing, Booking.new]) do |f| %>
    <%= f.select :start_time, @array_of_times, prompt: "Start" %>
    <%= f.select :end_time, @array_of_times[1..-1], prompt: 'End' %>
    <%= f.submit "Book"%>
<% end %>
<% if @listing.bookings.where(date: Date.today) %>
  <% @listing.bookings.where(date: Date.today).each do |booking| %>
    <p><%= booking.user.full_name %>: <%= booking.start_time %> to <%= booking.end_time %></p>
  <% end %>
<% end %>
